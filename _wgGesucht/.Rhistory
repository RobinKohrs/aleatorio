getSize = function(html){
size_node = html %>%
html_elements("h2.headline-key-facts") %>%
html_text() %>%
str_subset("m²")
if (length(size_node) == 0) {
return(NA_character_)
} else{
size = str_extract(size_node, "(\\d*)m") %>% gsub("m", "", .)
return(size)
}
}
